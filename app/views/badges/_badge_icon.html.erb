<svg width="<%= width %>" height="<%= height %>" viewBox="0 0 100 100" xmlns="http://www.w3.org/2000/svg">
    <style>
     text { font: bold 0.5rem sans-serif; };
    </style>

    <g
        <% if skill == 1 %>
            fill="#488b2c"
        <% elsif skill == 2 %>
            fill="#dc4720"
        <% else %>
            fill="#3f7ed1"
        <% end %>
    >
        <circle cx="50%" cy="50%" r="50%" />
        <circle cx="50%" cy="50%" r="45%" stroke="white" stroke-width="2" />
    </g>

    <line x1="6%" y1="50%" x2="94" y2="50%" stroke="black"/>

    <text x="50%" y="60%" fill="white" text-anchor="middle" >
        <% if name.nil? %>
            <tspan x="50%" dy="1.1em" font-size="0.6rem">nil</tspan>
        <% elsif name.chars.count > 18 %>
            <tspan x="50%" dy="1.1em" font-size="0.4rem"><%= name %></tspan>
        <% elsif name.chars.count > 16 %>
            <tspan x="50%" dy="1.1em" font-size="0.5rem"><%= name %></tspan>
        <% else %>
            <tspan x="50%" dy="1.1em" font-size="0.6rem"><%= name %></tspan>
        <% end %>
        <% if level == "none" %>
            <tspan x="50%" dy="1.1em" font-size="0.6rem">-</tspan>
        <% elsif level == "Beginner" %>
            <tspan x="50%" dy="1.1em" font-size="0.6rem">I</tspan>
        <% elsif level == "Intermediate" %>
            <tspan x="50%" dy="1.1em" font-size="0.6rem">II</tspan>
        <% else %>
            <tspan x="50%" dy="1.1em" font-size="0.6rem">III</tspan>
        <% end %>
    </text>

    <path d="m 6 50
             A 1 1 0 0 1 94 50" fill="black" />
    <% if I18n.locale == :en %>
        <image x="32.5%" y="20%" width="35%" height="25%" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="data:image/svg+xml;base64,"/>
    <% elsif I18n.locale == :fr %>
        <image x="32.5%" y="20%" width="35%" height="25%" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="data:image/svg+xml;base64,"/>
    <% else %>
        <image x="32.5%" y="20%" width="35%" height="25%" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="data:image/svg+xml;base64,"/>
    <% end %>
</svg>