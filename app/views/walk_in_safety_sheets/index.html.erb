<%= provide(:title, t('walk_in_safety_sheet.title')) %>
<h1 class="text-center"><%= t('walk_in_safety_sheet.title') %></h1>
<p class="fs-3 text-center">
    Select a space to sign the sheet for
</p>
<div class="container">
    <ul class="list-group">
        <% @spaces.each do |space| %>
            <%= link_to space.name, walk_in_safety_sheet_path(space.id),
                        class: "list-group-item list-group-item-action" %>
        <% end %>
    </ul>
</div>
