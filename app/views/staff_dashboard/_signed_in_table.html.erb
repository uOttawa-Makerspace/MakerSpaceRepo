<tbody id="table-js-signed-in">
    <% @space.signed_in_users.each do |user| %>
        <tr id="<%= return_no_waiver_id(user) %>" data-user-id=<%= user.id %>>
            <td>
                <%= link_to(tag.drop_username(user.name), user_url(user.username), class: "align-middle") %>
                <% if user.has_active_membership? %>
                    <% if user.active_membership.membership_tier.title_en.downcase.include?('faculty') %>
                        <p class="text-success"> <%= fa_icon "cogs" %>
                            Has an Engineering Membership</p>
                    <% elsif user.student? %>
                        <p class="text-success">
                            <%= fa_icon "university" %>
                            Has a Community Membership</p>
                    <% else %>
                        <p class="text-success">
                            <%= fa_icon "check-circle" %>
                            Has an External Membership</p>
                    <% end %>
                <% end %>
            </td>
            <td class="email-cell align-middle">
                <%= user.email %>
            </td>
            <td class="align-middle">
                <%= render 'staff_dashboard/flag', staff_dashboard_user: user %>
                <% if user.flagged? %>
                    <button type="button" class="btn btn-sm btn-danger" data-bs-toggle="modal" data-bs-target="#flag_message_<%= user.id %>">See Flag</button>
                <% else %>
                    None
                <% end %>
                <% if @space.needs_walk_in_safety_sheet? && !user.walk_in_safety_sheets.any? %>
                    <p class="text-danger"><%= fa_icon :warning %> Did not sign sheet</p>
                <% end %>
            </td>
            <td class="certifications-cell align-middle">
                <% proc = @space.ceed? ? @all_user_certs : @certifications_on_space %>
                <% if proc.call(user, @space.id).present? %>
                    <% proc.call(user, @space.id).each do |certification| %>
                        <div class="cert-name" style="display:inline">
                            <div class="cert-holder" style="display:inline">
                                <%= certification.training.localized_name %>
                                <div id="cert-details" style="display:inline">
                                    <%= certification.training.localized_name %> <br>
                                    <%= certification.trainer.name %> <br>
                                    <%= certification.updated_at.strftime('%B %d, %Y') %>
                                </div>
                                <br/>
                            </div>
                        </div>
                    <% end %>
                <% end %>
            </td>
            <% if @space.makerspace? %>
                <td class="printers-cell align-middle">
                    <% @printers_in_use.where(user_id: user.id).find_each do |printer_session| %>
                        <p class="m-0"><%= printer_session.printer.name %></p>
                    <% end %>
                </td>
            <% end %>
            <td class="email-cell align-middle">
                <%= user.lab_sessions.where('space_id' => @space.id).where('created_at BETWEEN ? AND ? ', 2.months.ago, DateTime.now).count %>
            </td>
            <td class="sign_out_time-cell align-middle" style="horizontal-alginment:center;">
                <%= time_ago_in_words(user.lab_sessions.sort_by(&:sign_out_time).second_to_last&.sign_out_time) + " ago at #{user.lab_sessions.last.space.name}" if user.lab_sessions.second_to_last.present? %></td>
            </td>
            <td class="align-middle dt-center">
                <%= button_to fa_icon('times'), { controller: :staff_dashboard, action: :sign_out_users, dropped_users: [user.username] }, method: :put, class: 'btn btn-default x-button', remote: true %>
            </td>
        </tr>
    <% end %>
</tbody>
<br />
